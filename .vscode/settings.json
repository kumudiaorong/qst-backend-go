{
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": true,
        "scminput": false,
        "c": true,
        "go": true
    },
    "window.zoomLevel": 1,
    "protoc": {
        "path": "protoc",
        "compile_on_save": false,
        "options": [
            "--proto_path=protos/v3",
            "--proto_path=protos/v2",
            "--proto_path=${workspaceRoot}/qst-proto/src",
            "--proto_path=${env.GOPATH}/src",
        ]
    }
}